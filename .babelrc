{
  
  "env": {
    "cjs": {
      "presets": [
        ["es2015", {"loose": true}]
      ],
      "plugins": [
        "add-module-exports",
        "transform-object-rest-spread"
      ]
    },
    "es": {
      "presets": [
        ["es2015", {"loose": true, "modules": false}]
      ],
      "plugins": [
        "transform-object-rest-spread"
      ]
    }
  }
}
